{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],

    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-empty": 1,
        "no-irregular-whitespace": 1,
        "linebreak-style": 0,
        "arrow-spacing": 0,
        "no-unused-vars": "off",
        "padded-blocks": "off",
        "indent": "off",
        "eol-last": 0,
        "no-console":0
    }
}